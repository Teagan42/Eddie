{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "rootDir": "./src",
    "outDir": "dist/",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "paths": {
      "@/auth": [
        "src/auth/*"
      ],
      "@/auth/*": [
        "src/auth/*"
      ],
      "@/api": [
        "src/api/*"
      ],
      "@/api/*": [
        "src/api/*"
      ],
      "@/chat": [
        "pages/chat/*"
      ],
      "@/chat/*": [
        "pages/chat/*"
      ],
      "@/config": [
        "src/pages/config/*,"
      ],
      "@/config/*": [
        "src/pages/config/*,"
      ],
      "@/styles": [
        "src/styles/*"
      ],
      "@/styles/*": [
        "src/styles/*"
      ],
      "@/": [
        "./src/*"
      ],
      "@/*": [
        "./src/*"
      ],
      "@shikijs/transformers": [
        "src/vendor/shiki-transformers.ts"
      ]
    },
    "types": [
      "vite/client",
      "vitest"
    ],
    "noEmit": true
  },
  "include": [
    "src",
  ],
  "references": [
    {
      "path": "../../platform/ui"
    }
  ]
}