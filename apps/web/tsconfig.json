{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "rootDir": "./src",
    "outDir": "dist/",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "paths": {
      "@/components/*": [
        "components/*"
      ],
      "@/vendor/*": [
        "vendor/*"
      ],
      "@/config": [
        "config/env.ts"
      ],
      "@/auth/*": [
        "auth/*"
      ],
      "@/api/*": [
        "api/*"
      ],
      "@/*": [
        "./src/*"
      ]
    },
    "types": [
      "vite/client",
      "vitest"
    ],
    "noEmit": true
  },
  "include": [
    "src",
    "vitest.config.ts",
    "playwright.config.ts",
    "tailwind.config.ts"
  ],
  "references": [
    {
      "path": "../../platform/ui"
    }
  ]
}