{
  "name": "@eddie/api",
  "version": "1.0.0",
  "private": true,
  "main": "dist/main.js",
  "type": "commonjs",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "prebuild": "tsc -b tsconfig.build.json",
    "build": "nest build api",
    "lint": "eslint 'src/**/*.ts'",
    "test": "vitest --run --config vitest.config.ts",
    "prestart": "npm run build",
    "start": "node dist/main.js",
    "build:watch": "tsc -b tsconfig.build.json --watch",
    "dev": "bash -c 'npm run build:watch -- --preserveWatchOutput & BUILD_PID=$!; trap \"kill $BUILD_PID\" EXIT; nest start api --watch'",
    "openapi:spec": "node -r ts-node/register ./src/openapi.ts"
  },
  "dependencies": {
    "@eddie/config": "^0.0.0",
    "@eddie/context": "^0.0.0",
    "@eddie/engine": "^0.0.0",
    "@eddie/io": "^0.0.0",
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/platform-socket.io": "^11.0.0",
    "@nestjs/websockets": "^11.0.0",
    "@nestjs/swagger": "^11.0.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.50.0",
    "@opentelemetry/resources": "^1.29.0",
    "@opentelemetry/sdk-node": "^0.52.1",
    "@opentelemetry/sdk-trace-base": "^1.29.0",
    "@opentelemetry/semantic-conventions": "^1.29.0",
    "on-finished": "^2.4.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/on-finished": "^2.3.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2"
  }
}
